{
    "manifest_version": 3,
    "name": "Activity Tracker",
    "version": "1.0",
    "description": "Track and manage your internet usage for better productivity.",
    "permissions": ["tabs", "storage", "webNavigation", "activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png"
      }
    }
  }
  